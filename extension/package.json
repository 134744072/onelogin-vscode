{
	"name": "olog",
	"displayName": "OneLogin OTP Generator",
	"description": "Generate OTP from Airbus account",
	"version": "0.0.1",
	"publisher": "yne",
    "engines": {
		"vscode": "^1.73.0"
	},
	"main": "./main.js",
	"extensionKind": [
		"workspace"
	],
	"activationEvents": [
		"onStartupFinished"
	],
    "contributes": {
		"commands": [
			{
				"command": "olog.login",
				"title": "Login OTP"
			},
			{
				"command": "olog.logout",
				"title": "Logout OTP"
			},
			{
				"command": "olog.generate",
				"title": "Generate OTP"
			}
        ]}
}